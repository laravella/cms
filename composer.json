{
    "name": "laravella/cms",
    "description": "Laravella CMS",
    "version": "v4.0.20140220",
    "keywords": ["laravel","cms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Geere",
            "email": "laravella.cms@gmail.com",
            "homepage": "http://github.com/victorgeere/laravella",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laravella/crud"
        }],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.x@dev"
    },
    "autoload": {
        "classmap": [
            "src/controllers",
            "src/migrations",
            "src/commands",
            "src/database/seeds"
        ],
        "files":[
            "src/Laravella/CMS/RavelHelpers.php"
        ],
        "psr-0": {
            "Laravella\\CMS": "src/"
        }
    },
    "minimum-stability": "dev"
}
